{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/bleech/gulp-starter.git"
  },
  "browser": {
    "jquery": "./src/assets/scripts/vendor/jquery-1.11.1.js",
    "plugin": "./src/assets/scripts/vendor/jquery-plugin.js",
    "modernizr": "./src/assets/scripts/vendor/modernizr-2.6.2-respond-1.1.0.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    },
    "modernizr": {
      "exports": "Modernizr"
    },
    "jquery": {
      "exports": "jQuery"
    }
  },
  "devDependencies": {
    "browser-sync": "^1.3.7",
    "browserify": "^5.10.1",
    "browserify-shim": "^3.7.0",
    "coffee-script": "^1.8.0",
    "coffeeify": "^0.7.0",
    "del": "^0.1.3",
    "gulp": "^3.8.10",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-jade": "^0.10.0",
    "gulp-notify": "^1.8.0",
    "gulp-stylus": "^1.3.4",
    "gulp-util": "^3.0.1",
    "handlebars": "^1.3.0",
    "hbsfy": "^2.0.0",
    "nib": "^1.0.4",
    "pretty-hrtime": "^0.2.2",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^1.0.6"
  }
}
